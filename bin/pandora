#!/usr/bin/env node
const program = require('commander')

program
  .version('0.1.0')

program
  .command('new <name>', 'creates a pandora project')

program
  .command('open [path]', 'opens jupyter lab')

program
  .command('list', 'lists previous projects')

program
  .command('clone', 'clones a previous project')

program
  .parse(process.argv)
